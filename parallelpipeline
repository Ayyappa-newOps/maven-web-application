pipeline{

agent any

tools{
maven 'maven-3.9.6'
}

//Checkout code
stages{
 stage('CheckOuteCode'){
 steps{
 git branch: 'development', changelog: false, credentialsId: '0f3d9fac-a704-4411-8b4a-d533f774307f', poll: false, url: 'https://github.com/uptimecareer/maven-web-application.git'
 }
 }
 //Build
 stage('ParallelExecution'){
   steps{
     parallel(
      Build: {
          sh "mvn clean package"
        },
      ExecuteSonarQubeReport: {
          sh "mvn clean sonar:sonar"
        }
       ) //parallel closing
      } //steps closing
     } //Parallelexecution closing
   } //Stages closing
} //pipeline closing
